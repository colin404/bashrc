alias on="export https_proxy=http://127.0.0.1:38457; export http_proxy=http://127.0.0.1:38457"
alias off="unset https_proxy;unset http_proxy"

alias dbm="mariadb -h 127.0.0.1 -P 3306 -uonex -p'onex(#)666'"
